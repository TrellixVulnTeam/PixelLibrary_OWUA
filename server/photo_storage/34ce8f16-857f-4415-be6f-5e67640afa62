<script>
(function () {
    var id = 'unsupportedBrowser-42441';
    function isCss3Browser() {
        // Проверим наличие CSS transform
        var props = ['transform', 'webkitTransform', 'OTransform', 'msTransform', 'MozTransform'];
        var div = document.createElement('div');
        for (var i = 0; i < props.length; i++) {
            var prop = props[i];
            if (prop in div.style) {
                return true;
            }            
        }

        return false;
    }
    function isIEWithFilters() {
        // Проверим наличие фильтров
        var div = document.createElement('div');
        return 'filters' in div;
    }

    if (!isCss3Browser() && !isIEWithFilters()) {
        $('#' + id).html('<div class="pe-error-message" style="margin-bottom: 8px;">В вашем браузере этот ' +
        'плейкаст может отображаться некорректно. В таком случае попробуйте просмотреть его более ' + 
        'современным браузером (Chrome, Safari или Firefox).</div>');
    }
})();    
</script>

