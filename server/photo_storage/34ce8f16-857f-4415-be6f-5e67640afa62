<script language="javascript">
var sendFormIframe = '<iframe src="/?module=forms&form=playcast_send_form" scrolling="no" frameborder="no" width="600" height="400" id="jFrameSend"><' + '/iframe>';


var isBBCodeLoaded = false;
function showCode(jObject, jType){

	if(jObject != null){

		showSend(null, 1);

		/*Начнем с того: "ну какой уже ie4?". Далее, getElementPosition отрабатывает неправильно, поэтому положение смещается по вертикали на много-много пикселей, НО на старой странице
		это перекрывалось другой ошибкой - новые значения положения не присваились элементу, потому все и отображалось правильно. Так что зачем это сейчас - мне не понятно
		/*switch (navigator.family)
		{
			case 'ie4':
				document.getElementById('getPlaycastCode').style.top = getElementPosition(jObject, 0);
				document.getElementById('getPlaycastCode').style.left = getElementPosition(jObject, 1) - 90;
			break;
			default:
				document.getElementById('getPlaycastCode').style.top = getElementPosition(jObject, 0);
				//$('#getPlaycastCode').css('top', getElementPosition(jObject, 0));
				//document.getElementById('getPlaycastCode').style.left = getElementPosition(jObject, 1) + 25;
				//$('#getPlaycastCode').css('left', getElementPosition(jObject, 1) + 25);
			break;
		}
        */
	}

	if(jType){
		document.getElementById('getPlaycastCode').style.display = 'none';
	}else{
		document.getElementById('getPlaycastCode').style.display = 'block';
	}

	if (!isBBCodeLoaded) {
		getBBCode(10344602, 'e1cb542f6d301c0535342107296e92c486dc55c7', $('#bbCode'));
	}

	return false;

}

function showSend(jObject, jType){

	if(jObject != null){

		showCode(null, 1);        

		switch (navigator.family)
		{
			case 'ie4':
				document.getElementById('getPlaycastSend').style.top = getElementPosition(jObject, 0) + 14;
				document.getElementById('getPlaycastSend').style.left = getElementPosition(jObject, 1) - 90;
			break;
			default:
				document.getElementById('getPlaycastSend').style.top = getElementPosition(jObject, 0) + 14;
				document.getElementById('getPlaycastSend').style.left = getElementPosition(jObject, 1) - 60;
			break;
		}
	}

	if(jType){
		document.getElementById('getPlaycastSend').style.display = 'none';
	}else{
        // Load iframe if it is not present
        if ($('#jFrameSend').length < 1) {
            $('#sendFormIframePlaceholder').html(sendFormIframe);
        }
		document.getElementById('getPlaycastSend').style.display = 'block';
	}

	return false;
}

</script>

