<script type="text/javascript">
    
    var setMatureStatus = 0;
    $(document).ready(function () {
        $('.premiuminfo').tooltip();
        window.published_loaded = false;

        var loaded = false;
        $(window).scroll(function () {
            if (loaded == false) {
                if ($(window).scrollTop() == $(document).height() - $(window).height()) {
                    this.loadingElement = $("<div><img src='/i/loading.gif'/> <span style='color: #f5f5f5;'>Идет загрузка...</span></div>");
                    $('#pc_content').append(this.loadingElement);
                    getTrainCasts();
                    loaded = true;
                }
            }
        });

        $("#openContestDialog").button().click(function () {
            $("#contest-dialog").dialog("open");
        });

        var savedUrl = $('body').css('background-image');
        var headerHeight = 0;
        var hasOverflowY = 'overflowY' in document.body.style;
        var overflowProperty = hasOverflowY ? 'overflow-y' : 'overflow';

        var imgTest = new Image;
        imgTest.src = '/i/btn_restore_grey.png';
        $('#fullView').click(function () {

            $('#sendFormIframePlaceholder').empty();  // Prevent reloading an iframe
            $(document.documentElement).removeClass('page-fullscreen').addClass('page-expanding');

            var jqTableMain = $('table.main');
            headerHeight = $('table.header').outerHeight(true);

            jqTableMain.css('margin-top', headerHeight + 'px');
            jqTableMain.animate({ 'margin-top': 0 },
                    { duration: 1000,
                        complete: function () {
                            $(document.documentElement).addClass('page-fullscreen').removeClass('page-expanding');
                            jqTableMain.css({ 'margin-top': ''});
                        }});

            if (parseInt($('#pcCommContainer').attr('data-is-not-empty'))) {
                $('#pcCommContainer').hide('blind', { }, 1000);
            }
            $('.playcastStat').hide('blind', { }, 1000);
            $('.playcastAuthor').hide('blind', { }, 1000);
            $('.playcastInfo').animate({ opacity: 0 }, { duration: 1000 });
            $('#anotherCasts').hide('blind', { }, 1000);
            $('#authorComment').hide('blind', { }, 1000);
        });
        $('#restoreView').click(function () {
            var jqTableMain = $('table.main');
            $(document.documentElement).removeClass('page-fullscreen').addClass('page-expanding');
            headerHeight = $('table.header').outerHeight(true);
            jqTableMain.animate({
                'margin-top': headerHeight
            }, { duration: 1000, complete: function () {
                jqTableMain.css({ 'margin-top': '' });
                $(document.documentElement).removeClass('page-expanding').removeClass('page-fullscreen');
                $('.header').hide().fadeIn(500);

            }});
            if (parseInt($('#pcCommContainer').attr('data-is-not-empty'))) {
                $('#pcCommContainer').show('blind', { }, 1000);
            }
            $('.playcastStat').show('blind', { }, 1000);
            $('.playcastAuthor').show('blind', { }, 1000);
            $('.playcastInfo').animate({ opacity: 1 }, { duration: 900 });
            $('#anotherCasts').show('blind', { }, 1000);
            $('#authorComment').show('blind', { }, 1000);
        });

        $('.setModerStamp3').click(function () {
            moderHandler3();
        });
        $('.setModerStamp2').click(function(){
            moderHandlerType2();
        });

    });

    var castList = '';
    var recomendList = '';
    var topViewList = '';

    function getTrainCasts() {
        $.getJSON("/", {
            module: "view_playcast_ajax_module",
            action: "getTrainCasts",
            castId: '10344602',
            userId: '547590'
        }, function (response) {
            if (response["success"]) {
                castsList = response["castsList"];
                recomendList = response["recomList"];
                topViewList = response["popularList"];
                if ($("#authorCastsSelector").length > 0) {
                    showAuthorCasts();
                } else {
                    showRecomendCasts();
                } //default
            }
        })
    }
    
</script>
